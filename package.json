{
  "name": "edb-node-pdf",
  "version": "0.14.0",
  "description": "Exemplo de uso do PDFKit com NodeJS.",
  "main": "index.js",
  "config": {
    "commitMessage": "Atualização de dependências"
  },
  "scripts": {
    "dev": "node index.js",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpu",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpu": "npm run gpo && npm run gpg && npm run gpb",
    "gpo": "git push origin master --tags",
    "gpg": "git push gitlab master --tags",
    "gpb": "git push bitbucket master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/edb-node-pdf.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "pdf",
    "pdfkit"
  ],
  "author": "Gabriel Bertola Bocca - gabriel at estudiodigitalbocca.com.br",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalbocca/edb-node-pdf/issues"
  },
  "homepage": "https://github.com/digitalbocca/edb-node-pdf#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "pdfkit": "^0.8.3",
    "svg-to-pdfkit": "^0.1.7"
  },
  "devDependencies": {
    "cross-var-no-babel": "^1.2.0",
    "standard": "^12.0.1"
  }
}
