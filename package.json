{
  "name": "edb-node-pdf",
  "version": "0.5.1",
  "description": "Exemplo de uso do PDFKit com NodeJS.",
  "main": "index.js",
  "config": {
    "commitMessage": "Correção nas dependências"
  },
  "scripts": {
    "dev": "node index.js",
    "git": "npm run gitstatus && npm run gitadd && npm run gitcommit && npm run gittag && npm run gporigin && npm run gpgitlab && npm run gpbitbucket",
    "gitstatus": "git status",
    "gitadd": "git add .",
    "gittag": "git tag v%npm_package_version%",
    "gitcommit": "git commit -m \"%npm_package_config_commitMessage%\"",
    "gporigin": "git push origin master --tags",
    "gpgitlab": "git push gitlab master --tags",
    "gpbitbucket": "git push bitbucket master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/edb-node-pdf.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "pdf",
    "pdfkit"
  ],
  "author": "Gabriel Bertola Bocca - gabriel at estudiodigitalbocca.com.br",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalbocca/edb-node-pdf/issues"
  },
  "homepage": "https://github.com/digitalbocca/edb-node-pdf#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "pdfkit": "^0.8.3",
    "svg-to-pdfkit": "^0.1.4"
  },
  "devDependencies": {
    "standard": "^10.0.3"
  }
}
